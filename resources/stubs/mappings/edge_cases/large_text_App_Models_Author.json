{
    "name": "Large Text Edge Cases Test"
    "description": "Test mapping for handling large text fields with various content types (short, medium, long, very long, numbers, special formats, URLs, code samples, mixed content)"
    "source_schema": {
        "columns": {
            "name": {
                "name": "name"
                "type": "string"
                "null_count": 0
                "unique_count": 10
                "examples": [
                    "Short Bio"
                    "Medium Bio"
                    "Long Bio"
                ]
            }
            "email": {
                "name": "email"
                "type": "email"
                "null_count": 0
                "unique_count": 10
                "examples": [
                    "short@test.com"
                    "medium@test.com"
                    "long@test.com"
                ]
            }
            "country": {
                "name": "country"
                "type": "string"
                "null_count": 0
                "unique_count": 10
                "examples": [
                    "USA"
                    "UK"
                    "Canada"
                ]
            }
            "bio": {
                "name": "bio"
                "type": "string"
                "null_count": 0
                "unique_count": 10
                "examples": [
                    "Brief bio"
                    "This is a medium length biography..."
                    "Lorem ipsum dolor sit amet..."
                ]
            }
        }
        "total_rows": 10
    }
    "mappings": [
        {
            "model": "App\\Models\\Author"
            "columns": [
                {
                    "source": "name"
                    "target": "name"
                    "transforms": [
                        "trim"
                    ]
                    "default": null
                    "validation_rule": "required|string|min:2|max:100"
                }
                {
                    "source": "email"
                    "target": "email"
                    "transforms": [
                        "trim"
                        "lower"
                    ]
                    "default": null
                    "validation_rule": "nullable|email|unique:authors,email"
                }
                {
                    "source": "country"
                    "target": "country"
                    "transforms": [
                        "trim"
                        "upper"
                        "truncate:2"
                    ]
                    "default": null
                    "validation_rule": "nullable|string|size:2|alpha"
                }
            ]
            "options": {
                "unique_key": "email"
                "duplicate_strategy": "update"
            }
        }
    ]
    "created_at": "2025-12-18T15:00:00+00:00"
    "updated_at": "2025-12-18T15:00:00+00:00"
}

